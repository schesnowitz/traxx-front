<p id="notice"><%= notice %></p>

<h1>Driver Applications</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Dob</th>
      <th>Social</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Postal</th>
      <th>Phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @driver_applications.each do |driver_application| %>
      <tr>
        <td><%= driver_application.first_name %></td>
        <td><%= driver_application.last_name %></td>
        <td><%= driver_application.dob %></td>
        <td><%= driver_application.social %></td>
        <td><%= driver_application.street %></td>
        <td><%= driver_application.city %></td>
        <td><%= driver_application.state %></td>
        <td><%= driver_application.postal %></td>
        <td><%= driver_application.phone %></td>
        <td><%= driver_application.email %></td>
        <td><%= link_to 'Show', driver_application %></td>
        <td><%= link_to 'Edit', edit_driver_application_path(driver_application) %></td>
        <td><%= link_to 'Destroy', driver_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Driver Application', new_driver_application_path %>
