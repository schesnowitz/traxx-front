<!-- begin #quote -->
<div id="quote" class="content bg-black-darker has-bg" data-scrollview="true">
    <!-- begin content-bg -->
    <div class="content-bg">
      <img src="#" alt="Quote" />
    </div>
    <!-- end content-bg -->
    <!-- begin container -->
    <div class="container" data-animation="true" data-animation-type="fadeInLeft">
      <!-- begin row -->
      <div class="row">
        <!-- begin col-12 -->
        <div class="col-md-12 quote">
          <i class="fa fa-quote-left"></i> <%= @page.string61 %> <br />
          <%= @page.string62 %><span class="text-theme"><%= @page.string63 %></span>
          <i class="fa fa-quote-right"></i>
          <small><%= @page.string64 %></small>
        </div>
        <!-- end col-12 -->
      </div>
      <!-- end row -->
    </div>
    <!-- end container -->
  </div>
  <!-- end #quote -->